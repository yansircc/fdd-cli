{
	"name": "fdd-cli",
	"version": "0.2.2",
	"description": "Feedback-Driven Development CLI - Compile fixes into triggerable pitfalls",
	"type": "module",
	"bin": {
		"fdd": "./dist/index.js"
	},
	"scripts": {
		"build": "bun build src/index.ts --outdir dist --target node && rm -rf templates && cp -r src/templates templates",
		"dev": "bun run src/index.ts",
		"test": "bun test",
		"lint": "biome check .",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"@inquirer/prompts": "^8.1.0",
		"chalk": "^5.3.0",
		"cli-table3": "^0.6.5",
		"commander": "^12.1.0",
		"gray-matter": "^4.0.3",
		"yaml": "^2.6.1"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@types/bun": "^1.2.0",
		"typescript": "^5.7.2"
	},
	"keywords": [
		"fdd",
		"feedback-driven-development",
		"ai",
		"claude",
		"pitfall",
		"experience-compiler"
	],
	"author": "yansircc",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/yansircc/fdd-cli.git"
	},
	"bugs": {
		"url": "https://github.com/yansircc/fdd-cli/issues"
	},
	"homepage": "https://github.com/yansircc/fdd-cli#readme",
	"engines": {
		"node": ">=18"
	},
	"files": [
		"dist",
		"templates"
	]
}
